<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
      <description></description>
      <servlet-name>cookieTestServlet</servlet-name>
      <display-name>cookieTestServlet</display-name>
      <servlet-class>com.clearlove.servlet.cookieTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>cookieTestServlet</servlet-name>
      <url-pattern>/xxx/ooo/jjj/cookieTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <description></description>
        <servlet-name>OtherServlet</servlet-name>
        <display-name>OtherServlet</display-name>
        <servlet-class>com.clearlove.servlet.OtherServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OtherServlet</servlet-name>
        <url-pattern>/xxx/ooo/jjj/other</url-pattern>
    </servlet-mapping>
</web-app>